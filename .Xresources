! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
!                  Background   Foreground  Background Alternate    Highlight Background    Highlight Foreground
rofi.color-window: #131620,     #c7a57c,    #131620
rofi.color-normal: #131620,     #c7a57c,    #131620,                #7c7d7c,                #131620
rofi.color-active: #131620,     #c7a57c,    #131620,                #7c7d7c,                #131620
rofi.color-urgent: #FF5555,     #070015,    #FF5555,                #7c7d7c,                #131620
rofi.terminal: urxvt
rofi.fuzzy: true
rofi.kb-cancel: Escape,Control+c
rofi.hide-scrollbar: true
rofi.separator-style: dash
rofi.fake-transparency: false
rofi.auto-select: true


! ------------------------------------------------------------------------------
! URxvt Color theme
! ------------------------------------------------------------------------------

URxvt*font: xft:Source Code Pro:pixelsize=13:antialiasing=true:size=12
URxvt*letterspace: 0
URxvt*scrollBar: false
URxvt.intensityStyles: false
URxvt.transparent:   false
URxvt.depth: 24

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! special
*.foreground:   #ffffff
*.background:   #1e1e1e
*.cursorColor:  #b22222

! black
*.color0:       #222222
*.color8:       #444444

! red
*.color1:       #ffffff
*.color9:       #ffffff

! green
*.color2:       #7c7d7c
*.color10:      #7c7d7c

! yellow
*.color3:       #ca4a4b
*.color11:      #ca4a4b

! blue
*.color4:       #ae0000
*.color12:      #ae0000

! magenta
*.color5:       #7c7d7c
*.color13:      #7c7d7c

! cyan
*.color6:       #b22222
*.color14:      #b22222

! white
*.color7:       #ffffff
*.color15:      #ffffff

URxvt.perl-ext-common: default,matcher,fullscreen,clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.secondaryWheel: 1
URxvt.internalBorder:  5
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

! macros for clipboard and selection
URxvt.copyCommand:      xclip -i -selection clipboard
URxvt.pasteCommand:     xclip -o -selection clipboard
URxvt.keysym.M-c:       perl:clipboard:copy
URxvt.keysym.m-v:       perl:clipboard:paste
URxvt.keysym.M-C-v:     perl:clipboard:paste_escaped
URxvt.keysym.M-Escape:  perl:keyboard-select:activate
URxvt.keysym.M-s:       perl:keyboard-select:search
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.urlLauncher:      firefox
URxvt.underlineURLs:    true
URxvt.urlButton:        1
URxvt.geometry:         90x30

! tabbed
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 16
URxvt.tabbed.tab-fg:    15
URxvt.tabbed.tab-bg:    4
URxvt*buffered:         false




! ------------------------------------------------------------------------------
! UXTerm standard archlabs theme
! ------------------------------------------------------------------------------



! termtype
UXTerm*termName: xterm-256color


UXTerm*cursorColor:        white
UXTerm*VT100.geometry:     90x30
UXTerm*font:               Monospace
UXTerm*faceSize:           12
UXTerm*dynamicColors:      true
UXTerm*utf8:               2
UXTerm*eightBitInput:      true
UXTerm*saveLines:          10000
UXTerm*scrollKey:          true
UXTerm*scrollTtyOutput:    false
UXTerm*scrollBar:          false
UXTerm*rightScrollBar:     false
UXTerm*jumpScroll:         true
UXTerm*multiScroll:        true
UXTerm*toolBar:            false

! fix colors
UXTerm*background: rgb:63/63/63
UXTerm*foreground: rgb:00/00/00
UXTerm*color0: rgb:00/00/00
UXTerm*color1: rgb:a8/00/00
UXTerm*color2: rgb:00/a8/00
UXTerm*color3: rgb:a8/54/00
UXTerm*color4: rgb:00/00/a8
UXTerm*color5: rgb:a8/00/a8
UXTerm*color6: rgb:00/a8/a8
UXTerm*color7: rgb:a8/a8/a8
UXTerm*color8: rgb:54/54/54
UXTerm*color9: rgb:fc/54/54
UXTerm*color10: rgb:54/fc/54
UXTerm*color11: rgb:fc/fc/54
UXTerm*color12: rgb:54/54/fc
UXTerm*color13: rgb:fc/54/fc
UXTerm*color14: rgb:54/fc/fc
UXTerm*color15: rgb:fc/fc/fc
UXTerm*boldMode: false
UXTerm*colorBDMode: true
UXTerm*colorBD: rgb:3f/3f/3f
